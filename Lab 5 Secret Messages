#include <stdio.h>
#include <string.h>
void bubs(char a[],int n){
    for(int i=0;i<n-1;i++){
        for(int j=0;j<n-1-i;j++){
            if(a[j]>a[j+1]){
                //swap
                char temp=a[j];
                a[j]=a[j+1];
                a[j+1]=temp;
            }
        }
    }
}
int main(){
    int n;
    scanf("%d",&n);
    char a[n+1];
    scanf("%s",a);

    char s[n+1];

    //sort
    bubs(a,n);

    char min=a[0];
    s[0]=min;
    a[0]='\0';
    
    int x=1;
    for(int i=1;i<n;i++){
        if(a[i]>min){
            s[x]=a[i];
            x++;
            
            min=a[i];
            
            a[i]='\0';
        }
        
    }
    
    for(int i=n;i>0;i--){
        if(a[i]<min&&a[i]!='\0'){
            
            s[x]=a[i];
            
            x++;
            min=a[i];
            a[i]='\0';
        }
        
    }
    printf("%s\n",s);
    return 0;
}
